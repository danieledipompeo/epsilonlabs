<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.emf.ecore.extension_parser">
      <parser
            class="org.eclipse.epsilon.emc.bibtex.impl.BibtexResourceFactoryImpl"
            type="bib">
      </parser>
   </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated Bibtex -->
      <package
            uri="https://www.eclipse.org/epsilon/0.1/bibtex"
            class="org.eclipse.epsilon.emc.bibtex.BibtexPackage"
            genModel="model/Bibtex.genmodel"/>
   </extension>

</plugin>
